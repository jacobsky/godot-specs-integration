; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="godot-specs-integration"
boot_splash/bg_color=Color( 0.12549, 0.145098, 0.192157, 1 )
config/icon="res://icon.png"

[global]

default=false
thread=false

[memory]

limits/multithreaded_server/rid_pool_prealloc=500
limits/message_queue/max_size_kb=65536
limits/command_queue/multithreading_queue_size_kb=65536

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
threads/thread_model=0
2d/opengl/batching_stream=2
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
